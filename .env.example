# Copy this file to .env and replace placeholder values.
# Never commit real credentials.

ACJI_ENV=dev

# Austin, TX center point
AUSTIN_LAT=30.2672
AUSTIN_LON=-97.7431
SEARCH_RADIUS_METERS=20000

# Public APIs
OVERPASS_ENDPOINT=https://overpass-api.de/api/interpreter
NOMINATIM_ENDPOINT=https://nominatim.openstreetmap.org/search
ACJI_HTTP_USER_AGENT=ACJI/0.1 (your-email@example.com)
NOMINATIM_REQUEST_DELAY_SECONDS=1.0
FOURSQUARE_API_KEY=your_foursquare_api_key_here

# Local warehouse and output paths
DUCKDB_PATH=./data/warehouse/acji.duckdb
GE_RESULTS_DIR=./data/quality
MODEL_ARTIFACT_PATH=./models/revenue_model.joblib
MODEL_METRICS_PATH=./models/revenue_metrics.json

# dbt
DBT_PROJECT_DIR=./dbt
DBT_PROFILES_DIR=./dbt
DBT_TARGET=dev
